package demo;

public class Test {

	public static void main(String[] args) {
		
		
		[]int arr= {2,4,5,6,3,2};
		
		   private static int[] findTwoSum_BruteForce(int[] nums, int target) {
		        for (int i = 0; i < nums.length; i++) {
		            for (int j = i + 1; j < nums.length; j++) {
		                if (nums[i] + nums[j] == target) {
		                    return new int[] { i, j };
		                }

	}

}
